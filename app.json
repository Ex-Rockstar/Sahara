{
  "expo": {
    "name": "Sahara",
    "slug": "sahara",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.exrockstar.sahara"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "scheme": "sahara",
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "dcbb9954-ecca-4c04-80f7-4ee03dfd81f9"
      }
    },
    "owner": "ex-rockstar"
  }
}
